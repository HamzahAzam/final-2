<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="course.java.ucl.loginactivity">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivityFragmentContainer"
            android:label="@string/app_name"
            android:windowSoftInputMode="adjustPan">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".QuizActivity"
            android:label="@string/app_name" />
        <activity
            android:name=".CheatActivity"
            android:label="@string/app_name" />
        <activity android:name=".SkipActivity" />
        <activity android:name=".Score">
            android:label="Super Quiz" />
        </activity>
        <activity android:name=".HighScores"></activity>
    </application>

</manifest>